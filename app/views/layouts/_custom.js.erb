function load_notification () {
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
      $("#notifications").html(this.responseText);
    }
  };
  xhttp.open("GET", "<%= notification_path %>", true);
  xhttp.send();
  $("#noti_number").hide();
}
