<div class="user-post user-post-<%= post.id %>", value="<% post.id %>">
  <div class="user-post-stat user-post-stat-<%= post.id %>">
    <div class="user-post-stat-component">
      <%= image_tag "resources/likefill.png" %>
      <p><%= post.likes.count %></p>
    </div>
    <div class="user-post-stat-component">
      <%= image_tag "resources/commentfill.png" %>
      <p><%= post.likes.count %></p>
    </div>
  </div>
  <%= link_to image_tag(post.photos.first.image.url), post,
    class: "user-post-image", value: post.id, remote: true %>
</div>

